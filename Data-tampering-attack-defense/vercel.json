{
    "version": 2,
    "builds": [{
            "src": "./index.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [{
            "src": "/check",
            "dest": "index.js"
        },
        {
            "src": "/create-checkout-session",
            "dest": "index.js"
        },
        {
            "src": "/",
            "dest": "public/index.html"
        },
        {
            "src": "/(.+)",
            "dest": "public/$1"
        }
    ]
}